<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="FishFarmDb"
         connectionString="Server=localhost\SQLEXPRESS;Database=FishFarmDB;Trusted_Connection=True;TrustServerCertificate=True;"
         providerName="System.Data.SqlClient" />
  </connectionStrings>

  <appSettings>
    <!-- Window dimensions -->
    <add key="MainWindow.Width" value="800" />
    <add key="MainWindow.Height" value="600" />

    <add key="TransferForm.Width" value="700" />
    <add key="TransferForm.Height" value="500" />

    <add key="StockingForm.Width" value="700" />
    <add key="StockingForm.Height" value="500" />

    <add key="MortalityForm.Width" value="700" />
    <add key="MortalityForm.Height" value="500" />

    <add key="CageForm.Width" value="700" />
    <add key="CageForm.Height" value="500" />

    <add key="BalanceForm.Width" value="700" />
    <add key="BalanceForm.Height" value="500" />

    <add key="MortalityPivotForm.Width" value="700" />
    <add key="MortalityPivotForm.Height" value="500" />
  </appSettings>
</configuration>
